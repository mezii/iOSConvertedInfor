  <script>
   
    $("#alert").hide();
    $("#signinButton").click(async function(){
        const email = $("#email").val();
        const password = $("#password").val();
        const result = await fetch('/user/login', {
            method: 'POST',
            headers: {
              'Content-Type': 'application/json'
            },
            body: JSON.stringify({
              email,
              password
            })
          }).then((res) => res.json())
        if (result.status === 'ok') {
           $.cookie("token",result.data);
           $.cookie('email', result.email)
           window.location.href = "dashboard"
        } else {
          $("#alert").text("Incorrect username or password");
          $("#alert").show();
        }
    })
    
  </script>
