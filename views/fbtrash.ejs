<script>

$("#upload").click(function(){
    
    var formData = new FormData($('#formFile')[0]);

    $.ajax({
        url: '/fbaccount/trash',
        data: formData,
        contentType: false,
        processData: false,
        cache: false,
        type: 'POST',
        success: function (data) {
            $("#fbaccounts").val(data);
        },
    });
})


</script>

